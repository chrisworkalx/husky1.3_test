{
  "name": "husky1.3_test",
  "version": "2.0.0",
  "description": "测试husky1.3版本，pre-commit 包括 commit-message",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version",
    "dev": "webpack serve",
    "build": "webpack"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --ext .js",
      "git add -n"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisworkalx/husky1.3_test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisworkalx/husky1.3_test/issues"
  },
  "homepage": "https://github.com/chrisworkalx/husky1.3_test#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "echarts": "^4.2.1",
    "vue": "^2.6.10"
  }
}
